{
    "compilerOptions": {
      "declaration": false,
      "target": "ES2017",
      "module": "commonjs", 
      "lib": [ "ES2017" ],
      "moduleResolution": "node",
      "typeRoots": ["node_modules/@types"],
      "allowSyntheticDefaultImports": true,
      "outDir": "./dist",
      "rootDir": ".",
      "removeComments": true,
      "sourceMap": false,
      "strict": true,
    },
    "exclude": [ "node_modules" ],
    "include": [ "./" ]
  }
  